{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "project": ["src/**/*.ts"],
    "ignore": ["src/bin/**/*.ts", "src/polyfills.ts"],
    "ignoreDependencies": ["commander", "ts-node"],
    "rules": {
        "files": "warn",
        "classMembers": "warn",
        "types": "warn",
        "dependencies": "warn",
        "devDependencies": "warn"
    }
}
